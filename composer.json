{
    "name": "webinarium/linode-api",
    "description": "Linode API Client Library",
    "type": "library",
    "keywords": ["api", "linode"],
    "homepage": "https://github.com/webinarium/linode-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Rodygin",
            "homepage": "https://github.com/webinarium"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.8",
        "psr/http-message": "^2.0",
        "symfony/expression-language": "^6.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Linode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linode\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
